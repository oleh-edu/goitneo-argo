{
  "title": "AIOps Quality - Inference Overview",
  "panels": [
    {
      "type": "graph",
      "title": "Requests per minute",
      "targets": [
        {
          "expr": "sum(rate(inference_requests_total[1m]))",
          "legendFormat": "req/s"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "graph",
      "title": "Latency (p50/p90)",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(inference_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.9, sum(rate(inference_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p90"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "stat",
      "title": "Drift events (total)",
      "targets": [
        {
          "expr": "sum(drift_events_total)"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 6,
        "h": 6
      }
    }
  ],
  "schemaVersion": 36,
  "version": 1
}
